<robot name="dummy_robot">
  <link name="base_link">
    <visual>
      <geometry>
        <cylinder length="0.6" radius="0.2"/>
      </geometry>
    </visual>
  </link>
  <link name="end_link"/>

  <joint name="first" type="continuous">
    <parent link="base_link"/>
    <child link="end_link"/>
    <origin xyz="0.5 0.3 0" rpy="0 0 0"/>
    <axis xyz="-0.707 0.707 0"/>
  </joint>
</robot>